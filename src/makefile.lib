LIB = $(TOP)/../../lib

all ::
	../makemake.exe
	$(MAKE) -f makefile.sub
	../makemake.exe -2
	$(MAKE) -C dj64 final

MAKEFILE_LIB = 1
include $(TOP)/../makefile.inc

ifneq ($(wildcard makefile.oi),)
include makefile.oi
endif
ifneq ($(wildcard makefile.oio),)
include makefile.oio
endif

clean ::
	$(TOP)/../makemake.exe
	$(MAKE) -f makefile.sub SUBARGS=clean
	@-$(MISC) rm makefile.oi makefile.rf makefile.rfo
